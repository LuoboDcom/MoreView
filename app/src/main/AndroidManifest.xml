<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ys.libraryp.mylibraryproject" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.ShareTABActivity"
            android:label="@string/title_activity_share_tab"
            android:theme="@style/ShareTheme" >
        </activity>
        <activity
            android:name=".ui.HorizontalScrollviewActivity"
            android:label="@string/title_activity_horizontal_scrollview" >
        </activity>
        <activity
            android:name=".ui.DrawerLayoutActivity"
            android:label="@string/title_activity_drawer_layout" >
        </activity>
        <activity
            android:name=".ui.PullRefreshActivity"
            android:label="@string/title_activity_pull_refresh" >
        </activity>
        <activity
            android:name=".ui.SlideMenuActivity"
            android:label="@string/title_activity_slide_menu" >
        </activity>
    </application>

</manifest>
